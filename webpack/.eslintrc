{
  "env": {
    "node": true
  },
  "plugins": [
    "node",
    "security"
  ],
  "extends": [
    "plugin:node/recommended",
    "plugin:security/recommended"
  ],
  "rules": {
    "strict": "off",
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": true
    }],
    "node/no-unpublished-require": "off"
  }
}
