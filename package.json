{
  "name": "parcel-project",
  "version": "0.1.0",
  "author": "doasync",
  "description": "Create a project with Parcel, Babel 7, React, Flow, Linters, Jest",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/doasync/parcel-project.git"
  },
  "scripts": {
    "lint": "npm run lint-js && npm run lint-css",
    "lint-js": "eslint .",
    "lint-css": "stylelint '+(**/*.css|**/*.scss|**/*.js)'",
    "test": "jest",
    "test-watch": "jest --watch",
    "prestart": "rimraf .cache",
    "start": "parcel src/index.html --open --no-cache",
    "prebuild": "rimraf dist",
    "build": "parcel build src/index.html",
    "more": "make usage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "wp-build": "npm-run-all wp-build-css wp-build-js",
    "wp-build-js": "webpack --config webpack/webpack.prod.js --progress",
    "wp-build-css": "webpack --config webpack/webpack.css.js",
    "wp-build-info": "webpack --config webpack/webpack.info.js",
    "wp-start": "webpack-dev-server --config webpack/webpack.dev.js"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "core-js": "^3.0.1",
    "react": "^16.8.6",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.8.4",
    "styled-components": "^4.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-flowtype": "^3.8.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "flow-bin": "^0.98.1",
    "flow-typed": "^2.5.1",
    "http-server": "0.11.1",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.2.2",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.1.7",
    "node": "^12.2.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "prettier": "^1.17.1",
    "react-testing-library": "^7.0.0",
    "rimraf": "^2.6.3",
    "stylelint": "^10.0.1",
