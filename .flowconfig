[ignore]
<PROJECT_ROOT>/.temp
<PROJECT_ROOT>/.idea
<PROJECT_ROOT>/.cache
<PROJECT_ROOT>/coverage
<PROJECT_ROOT>/dist

[include]

[libs]

[lints]
all=warn

[declarations]

[strict]
nonstrict-import
unclear-type
untyped-import
untyped-type-import
unsafe-getters-setters
sketchy-null

[options]
emoji=true
include_warnings=true
module.use_strict=true
module.file_ext=.js
module.file_ext=.jsx
module.name_mapper='^~/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
suppress_comment= \\(.\\|\n\\)*\\$FlowIgnore
suppress_type=$FlowFixMe
